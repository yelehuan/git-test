  <Group caption="自选股">
    <CompContainer imageindex = "20" name="compUserReport" caption="报价" align="alClient">
      <DynamicReport name="dynamicreport" caption="动态报价" align="alClient" canzoom="true" focustype="ftMove" defreportname="Dynamic" processcode="true" relatecode="compUserReport">
        <PopupMenu key="Report" caption="报价菜单"/>
      </DynamicReport>
      <SectorToolBar name="SectorToolBar" caption="板块切换栏" sectorlist="[UserSector]" canclosebutton="false" showaddbutton="false" align="alBottom" height="20" canresize="false" relatecode="dynamicreport">	
			</SectorToolBar>
    </CompContainer>
  </Group>
