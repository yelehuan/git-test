          <KLineInfo name="klineInfo" caption="K线信息框" align="alTop" height="20" canresize="false"/>
          <KLineMainChart name="kline" caption="K线图" align="alClient" height="300" canresize="false" focustype="ftNone" defcode="600000.SH" relatecode="klinecontainer,klineInfo,klinetimeline,klineVolume,klineAmount" relateindex="TradeDate,TradeTime,NewPrice,PrevClose,TodayOpen,TodayHigh,TodayLow,Volume,TotalAmount,DeltaAmount,DeltaVolume,SuspensionFlag">
