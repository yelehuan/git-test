            </KLineMainChart>
            <KLineTimeLine name="klinetimeline" caption="时间轴" align="alBottom" height="18" canresize="false" focustype="ftNone"/>
          </Container>
          <KLineSubChart name="klinevolume" caption="K线量图指标" align="alClient" height="150" focustype="ftNone" indextype="1" canclose="false" bottommargin="10" showzeroline="true" relateindex="DeltaVolume,BidVolume1,BidVolume2"/>
          <KLineSubChart name="klineamount" caption="K线成交额指标" visible="false" align="alClient" height="150" focustype="ftNone" indextype="2" canclose="false" bottommargin="10" showzeroline="true" relateindex="DeltaAmount,BidPrice1,BidPrice2"/>
        </KLineContainer>
      </MinuteKLineContainer>
      <Content name="AutoContent" AutoKeyParam="ContentKey" Caption="股票指标" Align="alRight" width="200" focustype="ftNone"/>
    </CompContainer>
    <CompContainer name="compMinuteKLine2" imageindex = "2" caption="分时K线2" align="alClient" relatecode="ctMinuteKLine">
      <MinuteKLineContainer name="ctMinuteKLine" caption="分时K线切换容器" align="alClient" processkeydown="true" relatecomp="minutecontainer,klinecontainer" relatecode="TimeTrend,klinecontainer" defcode="600000.SH">
